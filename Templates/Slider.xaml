        Width="200"
        Margin="0,0,0,0"
        HorizontalAlignment="Left"
        VerticalAlignment="Top"
        Header="Header"
        IsDirectionReversed="False"
        IsEnabled="True"
        LargeChange="10"
        Maximum="100"
        Minimum="0"
        Orientation="Horizontal"
        SmallChange="1"
        StepFrequency="1"
        TickFrequency="10"
        TickPlacement="Outside"
        Value="0" />
